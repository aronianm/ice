var html = `<%=j render :partial => 'name_form' %>`

$("#form1").html(html)

console.log("Build 1 in create name")

$("#second-build").on('click', function(){
	var programName = $("#name").val()
	var url = $(this).data('url')
	$("#name").attr('readonly', true)

	$.ajax({
		url: url,
		method: 'GET',
		beforeSend: function(xhr) {
			xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))
		},
		data: {'programs': {'name': programName}},
		dataType: 'jsonp',
		success: function(data){
			$("#second-build").remove();
		}
	})
	
})